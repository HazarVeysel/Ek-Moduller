@model ExceleGetir.Controllers.BlackListController.ViewModel
<ol reversed>
    @foreach (var item in Model.BlacklistNotes.OrderByDescending(x=>x.NoteId))
    {

        <li>

            <span><b>Kaydeden:</b> @item.AddedUserId</span><br />
            <span><b>Tarih:</b> @item.AddedDate</span>
            <div class="row px-3">
                <div class="col-xs-9 notedetail" style="background-color: whitesmoke; overflow-y: scroll; overflow-x: hidden; width: 80%; height: 50px; float: left" data-id="@item.BlackListNote" id="liste">@item.BlackListNote</div> <div class="col-xs-2"><button type="button" class="btn btn-secondary btn-xs detailbutton" data-toggle="modal" data-target="#modal-default" style="height:50px">View</button></div>
            </div>
            <hr />
        </li>


    }
</ol>
